<h1>Reservations#show</h1>

<%= link_to("<< Back to List", reservations_path, :class => 'back-link') %>

<div class="reservations show">
  <h2>Show Reservation</h2>

  <table summary="reservation detail view">
    <tr>
      <th>ID</th>
      <td><%= @reservation.id %></td>
    </tr>
    <tr>
      <th>Event Description</th>
      <td><%= @reservation.event_description %></td>
    </tr>
    <tr>
      <th>Checkout Date</th>
      <td><%= @reservation.checkout_date%></td>
    </tr>
    <tr>
      <th>Checkin Date</th>
      <td><%= @reservation.checkin_date %></td>
    </tr>

  </table>

</div>
